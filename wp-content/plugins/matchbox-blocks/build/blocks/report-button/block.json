{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "kindling/report-button",
  "title": "Report Button",
  "description": "A block to display a link to the afterburn report.",
  "category": "text",
  "icon": "button",
  "keywords": [
    "report",
    "button",
    "link"
  ],
  "version": "1.0.0",
  "textdomain": "kindling",
  "attributes": {
    "text": {
      "type": "string",
      "default": "Afterburn Report"
    }
  },
  "supports": {
    "align": false,
    "multiple": true,
    "anchor": false
  },
  "usesContext": [
    "postId",
    "postType"
  ],
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}